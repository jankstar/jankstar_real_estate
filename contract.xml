    <data noupdate="1">

        <record model="account.journal" id="journal_rental_book">
            <field name="name">Rental Book</field>
            <field name="code">RE</field>
            <field name="type">revenue</field>
        </record>

        <record model="real_estate.contract.type" id="contract_rental_agreement_type">
            <field name="name">Rental agreement</field>
            <field name="prefix" >1</field>
            <field name='start_number'>1000</field>
            <field name="step_term">10</field>
            <field name="step_item">10</field>
            <field name="sequence">10</field>
            <field name="account_journal" ref="journal_rental_book"/> 
            <field name="invoice_type">out</field>
            <field name="types_of_use" eval="['residential']"/>
        </record>

        <record model="real_estate.contract.type" id="contract_parking_space_lease_agreement_type">
