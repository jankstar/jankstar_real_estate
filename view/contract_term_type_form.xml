    <group colspan="2" col="-1" id="m_type">
        <label name="default_quantity"/><field name="default_quantity" />
        <label name="m_type"/><field name="m_type" />
    </group>    
    <newline/>
    <group colspan="2" col="-1" id="rhythm">
        <label name="rhythm"/><field name="rhythm" />
        <label name="rhythm_type"/><field name="rhythm_type" />
    </group>
    <newline/>
    <label name="account"/><field name="account" />
</form>
