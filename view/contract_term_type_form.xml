<form col="6">
    <label name="compute_name"/><field name="compute_name" />
    <group colspan="2" col="-1" id="checkboxes">
        <label name="active"/>
        <field name="active" xexpand="0" width="25"/>
    </group>
    <newline/>
    <group colspan="2" col="-1" id="name_sequence">
        <label name="sequence"/><field name="sequence"/>
        <label name="name"/><field name="name"/>
    </group>
    <newline/>
    <label name="types_of_use"/><field name="types_of_use" yexpand="0"/>
